#project "Struct parameter demo"
    
#include "/lib/standard.whl"
    
struct Point
    number   x, y, z
ends
    
Point    point
    
proc printPoint(Point pt)
    printN(pt.x)
    printN(pt.y)
    printN(pt.z)
endp
    
proc main()
    set      point.x,                -5
    set      point.y,                9
    set      point.z,                -3
    Point *p
    set p, &point
    printPoint(*p)
endp