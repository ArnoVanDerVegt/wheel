#project "Jump less demo"

#include "/lib/standard.asm"

proc main()
    number  n                               ; Declare a local number variable...

    set     n,                      2       ; Set n to 0
    cmp     n,                      1       ; Compare n to 1
    jl     	myLabel                         ; jump to myLabel if n is less than 1
    printS("n >= 1")
myLabel:

endp