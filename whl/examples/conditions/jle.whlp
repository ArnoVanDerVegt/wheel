#project "Jump less or equal demo"

#include "/lib/standard.asm"

proc main()
    number  n                               ; Declare a local number variable...

    set     n,                      2       ; Set n to 0
    cmp     n,                      1       ; Compare n to 1
    jle		myLabel                         ; jump to myLabel if n is less or equal than 1
    printS("n > 1")
myLabel:

endp